TCPKeepALive yes
ServerAliveInterval 60
ControlMaster auto
ControlPath /tmp/%r@%h:%p
ControlPersist yes

Host *
  UseRoaming no
  IdentityFile ~/.ssh/id_rsa

Host kejadlen.net
  IdentityFile ~/.ssh/prgmr_rsa

Host keajdlen.console.xen.prgmr.com
  IdentityFile ~/.ssh/prgmr_rsa
  User kejadlen

Host 127.0.0.1
  StrictHostKeyChecking no
