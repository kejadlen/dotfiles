#!/usr/bin/env ruby

require 'tempfile'

plist = ARGF.read
Tempfile.create('info.plist') do |f|
  f << plist
  f.flush

  vars = `/usr/libexec/PlistBuddy -c "Print :variablesdontexport" #{f.path}`
  vars = vars.split("\n")[1..-2].map(&:strip)

  vars.each do |var|
    `/usr/libexec/PlistBuddy -c "Set :variables:#{var} ''" #{f.path}`
  end

  f.rewind
  puts f.read
end
